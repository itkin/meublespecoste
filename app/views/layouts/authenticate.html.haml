%html
  = stylesheet_link_tag 'jquery-ui-humanity/jquery-ui-1.8.11.custom', 'compiled/authenticate'
  = csrf_meta_tag
  %title ADMIN
%body
  #page
    #flash
      - unless notice.blank?
        .notice= notice
      - unless alert.blank?
        .error= alert
    #auth-box.ui-widget.ui-widget-content.ui-corner-all
      = yield
